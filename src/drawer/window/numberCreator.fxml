<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="drawer.window.Controller">
   <children>
      <VBox spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#openFile" text="open" />
         </children>
         <HBox.margin>
            <Insets right="30.0" />
         </HBox.margin>
      </VBox>
      <VBox fx:id="mainVBox" layoutX="200.0" layoutY="54.0" spacing="10.0">
         <children>
            <TextField fx:id="fileNameField" layoutX="226.0" layoutY="260.0" prefHeight="30.0" />
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#loadImage" text="load" />
                  <Button mnemonicParsing="false" onAction="#saveImage" text="save" />
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#clearImage" text="clear" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
